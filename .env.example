# ============================================================================
# SEARCH APIS
# ============================================================================

# Google Search
SERPAPI_API_KEY=your-serpapi-key

# Tavily Search
TAVILY_API_KEY=your-tavily-api-key

# ============================================================================
# MONITORING E OBSERVABILIDADE
# ============================================================================

# Weights & Biases
WANDB_API_KEY=your-wandb-api-key
WANDB_PROJECT=awesome-ai-pocs

# LangSmith (LangChain)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langchain-api-key
LANGCHAIN_PROJECT=awesome-ai-pocs

# ============================================================================
# CLOUD PROVIDERS
# ============================================================================

# AWS
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

# Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/gcp-service-account.json

# Azure
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/

# ============================================================================
# DATABASE CONFIGURATIONS
# ============================================================================

# Redis
REDIS_URL=redis://localhost:6379/0

# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/ai-4-dummies

# ============================================================================
# MODEL CONTEXT PROTOCOL (MCP)
# ============================================================================
MCP_SERVER_URL=http://localhost:8000
MCP_API_KEY=your-mcp-api-key

# ============================================================================
# SECURITY E GUARDRAILS
# ============================================================================

CONTENT_MODERATION_API_KEY=your-moderation-api-key

# ============================================================================
# APPLICATION SETTINGS
# ============================================================================

ENVIRONMENT=development  # development, staging, production

# Debug mode
DEBUG=true

# Logging level
LOG_LEVEL=INFO

# API Rate limits
RATE_LIMIT_PER_MINUTE=60

# ============================================================================
# FINE-TUNING E TREINAMENTO
# ============================================================================

# HuggingFace Hub
HF_TOKEN=your-huggingface-write-token

# MLflow tracking
MLFLOW_TRACKING_URI=http://localhost:5000

# ============================================================================
# MULTI-MODAL APIS
# ============================================================================

# Stability AI
